<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.seguimiento13.Controllers.NewPassController">
   <children>
      <Label fx:id="cajaLBL" layoutX="218.0" layoutY="35.0" text="CAJA FUERTE">
         <font>
            <Font size="28.0" />
         </font>
      </Label>
      <Label fx:id="firstLBL" layoutX="206.0" layoutY="91.0" text="Digite la combinación actual">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Label fx:id="secondLBL" layoutX="206.0" layoutY="218.0" text="Digite la nueva combinación">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <HBox layoutX="100.0" layoutY="121.0" prefHeight="76.0" prefWidth="395.0">
         <children>
            <TextField fx:id="firstActualTF" onKeyTyped="#onKey1" prefHeight="75.0" prefWidth="75.0" />
            <TextField fx:id="secondActualTF" onKeyTyped="#onKey1" prefHeight="75.0" prefWidth="75.0" />
            <TextField fx:id="thirdActualTF" onKeyTyped="#onKey1" prefHeight="75.0" prefWidth="75.0" />
            <TextField fx:id="fourthActualTF" onKeyTyped="#onKey1" prefHeight="75.0" prefWidth="75.0" />
            <TextField fx:id="fifthActualTF" onKeyTyped="#onKey1" prefHeight="75.0" prefWidth="75.0" />
            <TextField fx:id="sixthActualTF" onKeyTyped="#onKey1" prefHeight="75.0" prefWidth="75.0" />
         </children>
      </HBox>
      <HBox layoutX="100.0" layoutY="255.0" prefHeight="75.0" prefWidth="395.0">
         <children>
            <TextField fx:id="firstNewTF" onKeyTyped="#onKey2" prefHeight="75.0" prefWidth="75.0" />
            <TextField fx:id="seconNewTF" onKeyTyped="#onKey2" prefHeight="75.0" prefWidth="75.0" />
            <TextField fx:id="thirdNewTF" onKeyTyped="#onKey2" prefHeight="75.0" prefWidth="75.0" />
            <TextField fx:id="fourthNewTF" onKeyTyped="#onKey2" prefHeight="75.0" prefWidth="75.0" />
            <TextField fx:id="fifthNewTF" onKeyTyped="#onKey2" prefHeight="75.0" prefWidth="75.0" />
            <TextField fx:id="sixthNewTF" onKeyTyped="#onKey2" prefHeight="75.0" prefWidth="75.0" />
         </children>
      </HBox>
      <Button fx:id="changeBTN" layoutX="111.0" layoutY="340.0" mnemonicParsing="false" onAction="#change" prefHeight="40.0" prefWidth="106.0" style="-fx-background-color: #0000FF;" text="Cambiar clave" textFill="#f2eded" />
      <Button fx:id="closeBTN" layoutX="374.0" layoutY="340.0" mnemonicParsing="false" onAction="#close" prefHeight="40.0" prefWidth="106.0" style="-fx-background-color: #0000FF;" text="Cerrar caja" textFill="#faf6f6" />
   </children>
</AnchorPane>
